{
  "name": "@qmaru/mdp-js",
  "version": "1.1.0",
  "description": "js mdp image urls",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "mdp-js": "dist/cli.js"
  },
  "scripts": {
    "build:node": "bun build src/index.ts --outdir dist/node --target node --sourcemap=external",
    "build:web": "bun build src/index.ts --outdir dist/web --target browser --minify --sourcemap=external",
    "build:types": "bun run tsc --emitDeclarationOnly --outDir dist/types",
    "build": "bun run build:types && bun run build:node && bun run build:web",
    "test": "bun test",
    "lint": "oxlint",
    "fmt": "oxfmt"
  },
  "files": [
    "dist"
  ],
  "author": "qmaru",
  "license": "ISC",
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "node-html-parser": "^7.0.1"
  },
  "publishConfig": {
    "@qmaru:registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}